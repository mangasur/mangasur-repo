		"lint:pkg": "eslint -c eslint.config.mjs ./packages/**/*.ts",
		"lint:fix": "eslint -c eslint.config.mjs ./packages/**/*.ts --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mangasur/mangasur-repo.git"
	},
	"author": "FrierenDv",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mangasur/mangasur-repo/issues"
	},
	"homepage": "https://github.com/mangasur/mangasur-repo#readme",
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
		"@swc/jest": "^0.2.38",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.14.1",
		"dotenv": "^16.5.0",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-prettier": "^5.2.6",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"ts-jest": "^29.3.2",
		"ts-node": "^10.9.2",
		"tsup": "^8.4.0",
		"turbo": "^2.5.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.31.0"
